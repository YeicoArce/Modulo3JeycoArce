pipeline {
    agente any
    stages {
        @stages('Newman test') {
            steps {
                sh '''
                node --version
                cd Modulo3JeycoArce/ci
                newman run ProyectoJeycoArce.postman_collection.json 
                '''
            }
        }
        @stages('RestAssured test') {
            steps {
                sh '''
                cd Modulo3JeycoArce/RestAssured
                mvn clean test 
                '''
            }
        }
       
    }
}