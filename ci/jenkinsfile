pipeline {
    agent any
    stages {
        stage('Newman test') {
            steps {
                sh '''
                node --version
                newman run ProyectoJeycoArce.postman_collection.json -e ambiente_examen.postman_environment.json
                '''
            }
        }

        stage('RestAssured test pipeline') {
            steps {
                bat '''
                cd Modulo3JeycoArce/RestAssured
                mvn clean install 
                '''
            }
        }
       
    }
}