pipeline {
    agent any
    stages {
        stage('Newman test') {
            steps {
                bat '''
                node --version
                npm install -g newman
                cd Modulo3JeycoArce/ci
                newman run ProyectoJeycoArce.postman_collection.json 
                '''
            }
        }
        
        stage('RestAssured test pipeline') {
            steps {
                bat '''
                cd RestAssured
                mvn clean install 
                '''
            }
        }
       
    }
}